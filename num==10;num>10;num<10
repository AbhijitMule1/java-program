public class HelloWorld{

     public static void main(String []args){
         int a=11;
         if(a==10)
         
        System.out.println("a is exactly equal to 10");
        else if (a<10)
         System.out.println("a is less than 10");
         else if(a>10)
          System.out.println("a is greater than 10");
       
     }
}
