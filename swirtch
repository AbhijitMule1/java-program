package assingment;
import java.util.Scanner;
public class Switch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
    Scanner sc=new Scanner(System.in);
    System.out.println("enter your case:");
    int num=sc.next().charAt(0);
    switch(num) {
    case  1:
    	 int a=10;
    	 System.out.printf("decimal=%d",a);
    	 break;
    case 2:
    	      int b=20;
    	      System.out.printf("hexadecimal=%x",b);
    	       break;
    case 3:
    	   int c=230;
    	   System.out.printf("octal=%o",c);
    	   break;
    	   
      default :
               System.out.println("enter correct coice:");
 
    	      
    }
	}

}
